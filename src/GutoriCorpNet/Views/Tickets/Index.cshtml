@model IEnumerable<GutoriCorp.Data.Models.Ticket>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.created_by)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.created_on)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fine_amount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.modified_by)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.modified_on)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.occurrence_place)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.status_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ticket)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ticket_date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ticket_num)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.tlc_plate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.vin_code)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.created_by)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.created_on)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fine_amount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.modified_by)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.modified_on)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.occurrence_place)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.status_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ticket)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ticket_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ticket_num)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tlc_plate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.vin_code)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
